<% @preferred_shops.each do |pref| %>
   <tr>
        <td><%= pref.name %></td>
        <td><%= pref.distance %></td>
        <td><%= image_tag(pref.shop_image.url)  %></td>
        <td><%= button_to 'Remove', unlike_shop_path(pref), method: :put , remote: true, class: 'button  is-small is-danger' %></td>
    </tr>
<% end %>